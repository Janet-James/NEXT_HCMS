 <ul>
{{#if HISTORY}}
  {{#if HISTORY.length}}
    {{#each HISTORY}}
      {{#if dayFirstMsg}}
        {{{dayFirstMsg}}}
      {{/if}}
      {{#if mid}}
         <li class=message-data id={{mid}} data-rid={{rid}} data-timestamp={{ts}}>
            <div class="img-name">
               <a href="#">
                  <span>{{uname}}</span>
                  <img class="mem-cir-img" src={{ip uname}}>
                  <p>{{{msg}}}</p>
                  <span class="search-time">{{ct}}</span>
               </a>
               <div class="sent-editor-sec">
                  <a href="#"><i class="fa fa-ellipsis-v"></i></a>
                  <ul>
                     <li> <a class="jumpToMsgBtn" href="#" data-rid={{rid}} data-timestamp={{ts}}>Jump to Message</a> </li>
                  </ul>
               </div>
            </div>
         </li>
         {{/if}}
    {{/each}}
  {{/if}}
{{/if}}
</ul>
